{
  "name": "codeKata",
  "description": "Code-Kata",
  "keywords": [
    "framework"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.6.4"
  },
  "require-dev": {
    "mockery/mockery": "0.9.9",
    "phpunit/phpunit": "5.7.27"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "secure-http": false,
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
